{
    "name": "lucenarenato/lumen-sail",
    "description": "Installing Laravel Sail Package in Lumen - ^5.5|^6.0|^7.0",
    "license": "MIT",
    "support": {
        "source": "https://github.com/lucenarenato/lumen-sail"
    },
    "authors": [
        {
            "name": "Renato Lucena",
            "email": "cpdrenato@gmail.com"
        }
    ],
    "require": {
        "lucenarenato/sail-7.x": "1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "lucenarenato\\LumenSail\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "lucenarenato\\LumenSail\\LumenSailServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
